import java.io.*;
import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        try{
            int A = s.nextInt();
            int B = s.nextInt();
            int c = A/B;
            System.out.println(c);
            
        }
        catch(ArithmeticException e){
            System.out.println("Division by zero is not allowed");
        }
    }
}
