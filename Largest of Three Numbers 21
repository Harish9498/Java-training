import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int A = scanner.nextInt();
        int B = scanner.nextInt();
        int C = scanner.nextInt();

        int largest = A;

        if (B > largest) {
            largest = B;
        }

        if (C > largest) {
            largest = C;
        }

        System.out.println(largest);
    }
}
