import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int N = scanner.nextInt();
        int[] arr = new int[N];
        for (int i = 0; i < N; i++) {
            arr[i] = scanner.nextInt();
        }
        int maxElement = arr[0];
        for (int i = 1; i < N; i++) {
            if (arr[i] > maxElement) {
                maxElement = arr[i];
            }
        }
        System.out.println(maxElement);
        scanner.close();
    }
}
