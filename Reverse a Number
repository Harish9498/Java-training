import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner scanner = new Scanner(System.in);
    
        int N = scanner.nextInt();
        int reversedNum = 0;
        int temp = N;

        while (temp != 0) {
            int digit = temp % 10;
            reversedNum = reversedNum * 10 + digit;
            temp /= 10;
        }
        System.out.println(reversedNum);

        scanner.close();
    }
}
