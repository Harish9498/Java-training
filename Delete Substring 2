import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      
        Scanner s = new Scanner(System.in);
        String T = s.nextLine();
        StringBuilder sb = new StringBuilder (T);
        int x=s.nextInt();
        int y=s.nextInt();
        sb.delete(x,y);
        System.out.println(sb);
    }
}
      
