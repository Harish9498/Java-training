import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner scanner = new Scanner(System.in);
            String s = scanner.nextLine();
       
            if (s == null) {
                System.out.println("Null value found");
            } else {
                System.out.println(s.length());
            }
        }
    }
